{
  "private": true,
  "scripts": {
    "prepare": "husky",
    "dev": "next dev",
    "build": "next build",
    "build:prod": "dotenv -c production -- next build",
    "start": "next start",
    "theme": "npx tailwind-preset-mantine ./src/app/theme.ts -o ./src/styles/theme.css && npx prettier --write ./src/styles/theme.css",
    "lint": "next lint",
    "format": "prettier --list-different \"src/**/*.{ts,tsx,css}\" \"prisma/schema.prisma\"",
    "check-types": "tsc --noEmit",
    "start:prod": "dotenv -c production -- node ./.next/standalone/server.js",
    "db:gen": "pnpm db:generate",
    "db:generate": "prisma generate",
    "db:mi": "pnpm db:migrate",
    "db:migrate": "dotenv -c development -- prisma migrate dev",
    "db:push": "dotenv -c development -- prisma db push",
    "db:deploy": "dotenv -c production -- prisma migrate deploy",
    "db:reset": "dotenv -c development -- prisma migrate reset",
    "auth:gen": "pnpm auth:generate",
    "auth:generate": "npx @better-auth/cli generate && npx prettier --write ./prisma/schema.prisma"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.876.0",
    "@aws-sdk/s3-request-presigner": "^3.876.0",
